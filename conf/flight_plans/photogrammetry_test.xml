#ifdef DC_AUTOSHOOT_QUARTERSEC_PERIOD
#include "modules/digital_cam/dc.h"
#define LINE_START_FUNCTION dc_Survey(dc_gps_dist);
#define LINE_STOP_FUNCTION dc_autoshoot = DC_AUTOSHOOT_STOP;
#endif
-->
  </header>

  <waypoints>
    <waypoint name="HOME" x="15.0" y="5.0"/>
    <waypoint name="CR" x="0.0" y="200.0"/>
    <waypoint name="S1" x="-231" y="-161"/>
    <waypoint name="S2" x="-161" y="231"/>
    <waypoint name="S3" x="231"  y="161"/>
    <waypoint name="S4" x="161"  y="-231"/>
  </waypoints>
  
  <blocks>
    <block name="wait" strip_button="wait" strip_icon="home.png" group="home">
